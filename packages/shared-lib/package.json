  "overrides": {
    "csurf": {
      "cookie": "0.7.2"
    },
    "@ministryofjustice/hmpps-audit-client": {
      "fast-xml-parser": "^5.3.4"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=22"
  },
  "jest": {
    "resolver": "<rootDir>/jest-resolver.js",
    "transformIgnorePatterns": [
      "node_modules/(?!(uuid|@smithy|@aws-sdk|@ministryofjustice/hmpps-audit-client|@ministryofjustice/probation-search-frontend)/)"
    ],
    "moduleNameMapper": {
      "^uuid$": "<rootDir>/node_modules/uuid/dist/index.js"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ],
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,js,jsx,mjs}"
    ],
    "coveragePathIgnorePatterns": [
      ".*.test.ts",
      "node_modules",
      "integration_tests",
      "wiremock",
      "src/types",
      ".*jest.config.js",
      "src/index.ts",
      ".*.cy.ts"
    ],
    "coverageDirectory": "test_results/jest/",
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "testMatch": [
      "<rootDir>/(src|job)/**/?(*.)(cy|test).{ts,js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test_results/jest/"
        }
      ],
      [
        "./node_modules/jest-html-reporter",
        {
          "outputPath": "test_results/unit-test-reports.html"
        }
      ]
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "node",
      "ts"
    ]
  }
}
