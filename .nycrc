{
  "extends": "@istanbuljs/nyc-config-typescript",
   "check-coverage": true,
   "compact": false,
   "inPlace": true,
   "exclude": [
     "assets/**",
     "coverage/**",
     "packages/*/test{,s}/**",
     "**/*.d.ts",
     "test{,s}/**",
     "test{,-*}.{js,cjs,mjs,ts,tsx,jsx}",
     "**/*.cy.ts",
     "**/*{.,-}test.{js,cjs,mjs,ts,tsx,jsx}",
     "**/__tests__/**",
     "**/{ava,babel,nyc}.config.{js,cjs,mjs}",
     "**/jest.config.{js,cjs,mjs,ts}",
     "**/{karma,rollup,webpack}.config.js",
     "**/.{eslint,mocha}rc.{js,cjs}"
   ]
 }